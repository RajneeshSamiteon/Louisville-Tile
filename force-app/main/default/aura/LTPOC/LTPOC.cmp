<aura:component controller="LT_PreferredVendorRFPCtrl" 
                implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" 
                access="global" >
    
    <!-- Show Term And Conditions Modal on page load -->
    <aura:attribute name="termAndConditionmodalOpen" type="Boolean" default="true" />
    
    <aura:attribute name="sectionOpen" type="Boolean" default="false" />   
    <aura:attribute name="recordId" type="string" default="" />
    <aura:attribute name="filetype" type="List" default="['.mp4']"/>
    <aura:attribute name ="agreementObject" type = "Object" default=""/>
    <aura:attribute name="currentSection" type="string" default="" />
    <aura:attribute name="encryptedToken" type="String"/>
    
    <aura:attribute name="allSectionData" type="object"/>
    <aura:attribute name="allFileData" type="object"/>
    
    <!--For Showing Or Hide Modal-->
    <aura:attribute name="modalOpen" type="Boolean" default="false"/>
    
    <aura:attribute name="columns" type="List"/>
    
    <aura:attribute name="discontinuedSeries" type="List" default="[{'label': 'Notice given to the distributor', 'value': 'Notice given to the distributor'},
                                                                   {'label': 'Opportunity to buy-up remaining inventories of high turn items', 'value': 'Opportunity to buy-up remaining inventories of high turn items'},
                                                                   {'label': 'Buy back or trade out agreement for discontinued product', 'value': 'Buy back or trade out agreement for discontinued product'},
                                                                   {'label': 'Other', 'value': 'Other'}]"/>
    
    <aura:attribute name="priceIncreases" type="List" default="[{'label': 'Notice given to the distributor', 'value': 'Notice given to the distributor'},
                                                               {'label': 'Opportunity to purchase high turn items ', 'value': 'Opportunity to purchase high turn items '},
                                                               {'label': 'Other', 'value': 'Other'}]"/>
    
    <!--Start Accordion attributes-->
    <aura:attribute name="activeSections" type="List" default="['firmNameAddress','affiliates','firmHistory',
                                                               'contactInformation','dedicatedServiceDepartment',
                                                               'affiliateCompanies','manufacturingDetails',
                                                               'partnersInsurance','favoriteAccounts','insurance','WareHouse']" />     
    
    <aura:attribute name="activeSectionsB" type="List" default="['productGuarantees','shippingInformation',
                                                                'nationalAccount','privatelabel',
                                                                'financialConsiderations','VendorRebateProgram']" />
    <aura:attribute name="activeSectionsC" type="List" default="['strategicPlanning','marketingSupport',
                                                                'agreement','promotionSales','preferredPartner',
                                                                'productViability','companyPolicy']" /> 
    <aura:attribute name="activeSectionsD" type="List" default="['realEstate','marketingBudget',
                                                                'digitalLibrary','marketingEfforts',
                                                                'experienceProcess','merchandisingSupport','References']" />
    
    <aura:attribute name="activeSectionsE" type="List" default="['uptimePerformance','visualizationCapabilities',
                                                                'differentiateTechnology','onlineCapabilities']" />
    
    <aura:attribute name="activeSectionsF" type="List" default="['competitiveAdvantage']" />
    
    <aura:attribute name="referencesSection" type="List" default="['Reference1','Reference2','Reference3']" />
    
    <aura:attribute name="accordionName" type="Integer" default = "1"/> 
    <aura:attribute name="accountAccordionName" type="Integer" default = "1"/> 
    <aura:attribute name="contactAccordionName" type="Integer" default = "1"/>
    <!--End Accordion attributes-->
    
    <!--Start Affaliates attributes-->
    <aura:attribute name="isAffiliates" type="string"  />
    <aura:attribute name="affiliateDefault" type="object" default="{}" />
    <aura:attribute name="lstAffiliates" type="list"  default="[{}]"/>
    <aura:attribute name="lstAccounts" type="list"  default="[{}]"/>
    <aura:attribute name="deletedAffiliates" type="list"  default="[]"/>
    <aura:attribute name="affiliatesDefault" type="object"  default="{}"/>
    <!--End Affaliates attributes-->
    
    <!--Start Contact Information attributes-->
    <aura:attribute name="contactId" type="string" default="" />   
    <aura:attribute name="isContact" type="string"/>
    <aura:attribute name ="contacts" type = "Object" default=""/>
    <aura:attribute name="isWarehouse" type="string"/>
    <aura:attribute name="deletedKeyContact" type="list"  default="[]"/>
    <aura:attribute name="lstContacts" type="list"  default="[]"/>
    <aura:attribute name="contactDefault" type="object"  default="{}"/>    
    <!--End  Contact Information attributes-->
    
    <aura:attribute name="isBalanceSheet" type="string"/>
    <aura:attribute name="deletedcurrentTopAccount" type="list"  default="[]"/>
    <aura:attribute name="isOutSource" type="string"/>
    <aura:attribute name="accountDefault" type="object"  default="{}"/>
    
    <!--List of contentVersionIds -->
    <aura:attribute name="lstcontentVersionId" type="list"  default="[]"/>
    
    <!--List of States -->
    <aura:attribute name="lstState" type="list"  default="[]"/>
    
    <!--List of Department Servicing LT -->
    <aura:attribute name="lstDepartmentService" type="list"  default="[]"/>
    
    <!--List of Department Servicing LT -->
    <aura:attribute name="lstAffiliatesRec" type="list"  default="[]"/>
    
    <!--List of Account types -->
    <aura:attribute name="lstAccountType" type="list"  default="[]"/>
    
    <!--  Required for All fields -->
    <aura:attribute name = "requiredForAll" type = "boolean" default = "true"/>
    <aura:attribute name="isValid" type="Boolean" default="true" />
    
    <aura:attribute name="deleteIndex" type="List" />
    
    <aura:attribute name="sectionAWarehouse" type="List"/>
    <aura:attribute name="sectionAManufacturingDetails" type="List"/>
    <aura:attribute name="sectionBDistributor" type="List"/>
    <aura:attribute name="sectionBManufacturer" type="List" />
    <aura:attribute name="sectionBBalanceSheet" type="List" />
    <aura:attribute name="sectionBAgreement" type="List" />
    <aura:attribute name="sectionCStrategicPlanning" type="List" />
    
    <!--Start Next/Previous Button attributes-->
    <aura:attribute name="previousPageName" type="string"/>
    <aura:attribute name="nextPageName" type="string" default="sectionA"/>
    
    <!--End Next/Previous Button attributes-->
    <aura:attribute name="picklistOptions" type="list"  default="['Yes','No']"/>
    
    <!-- Spinner -->
    <aura:attribute name ="Spinner" type= "boolean" default = "false"/>
    
    <aura:attribute name="deleteFileRecordId" type="string" />
    
    <!-- Start Handlers -->
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <!-- End Handlers -->
    
    <!-- delete sample item EVENT handler-->
    <aura:handler name="deleteRecordEvent" event="c:deleteRecords" action="{!c.deleteRecordByIndex}"/>
    
    <aura:attribute name="masterSectionAWrapper" type="LT_PreferredVendorRFPCtrl.MasterSectionAWrapper"/>
    <aura:attribute name="masterSectionBWrapper" type="LT_PreferredVendorRFPCtrl.MasterSectionBWrapper"/>
    <aura:attribute name="masterSectionCWrapper" type="LT_PreferredVendorRFPCtrl.MasterSectionCWrapper"/>
    <aura:attribute name="masterSectionDWrapper" type="LT_PreferredVendorRFPCtrl.MasterSectionDWrapper"/>
    <aura:attribute name="masterSectionEWrapper" type="LT_PreferredVendorRFPCtrl.MasterSectionEWrapper"/>
    <aura:attribute name="masterSectionFWrapper" type="LT_PreferredVendorRFPCtrl.MasterSectionFWrapper"/>
    
    <aura:html tag="style">
        html {
        background: #f2f7f8;
        padding-left:2%;
        padding-right:2%;
        }
    </aura:html>
    <aura:if isTrue="{!!v.masterSectionAWrapper.isSubmit}">
        <aura:if isTrue="{!v.Spinner == true}">
            <lightning:spinner alternativeText="Loading" size="medium" />
        </aura:if>
        <!-- Heading of Form -->
        <div aura:id="container" class="{!v.nextPageName=='Complete'?'slds-card container completeBg':'slds-card container'}" style = "overflow-x:hidden; width:100% ; max-width:100%;" >
            <!--loading spinner start... style=Brand Medium (blue dots)   -->
            
            
            <!--  Loading spinner end--> 
            <!-- Louisville Tile Logo -->
            <c:LouisvilleTileLogoForVendor />
            
            <!--Modal Box For Terms And Conditions Of Sales Start-->
            <aura:if isTrue="{!v.termAndConditionmodalOpen}">
                <article class="slds-card">
                    <div class="slds-card__header slds-grid">
                        <header class="slds-media slds-media_center slds-has-flexi-truncate">
                            <div class="slds-media__body slds-align_absolute-center">
                                
                                <div class="slds-grid slds-grid_vertical">
                                    <h2 class="slds-card__header-title">
                                        <div class="slds-col slds-align_absolute-center slds-text-heading_medium">
                                            <span><u> Proposal Guidelines and Instructions</u></span>
                                        </div>
                                    </h2>
                                    <div class="slds-col slds-align_absolute-center slds-text-heading_medium">
                                        <span>Louisville Tile Distributors</span>
                                    </div>
                                    <div class="slds-col slds-align_absolute-center slds-text-heading_medium">
                                        <span>Request for Proposal Preferred Partner Vendor</span>
                                    </div>
                                    <div class="slds-col slds-align_absolute-center slds-text-heading_medium	">
                                        <span> March 1, 2022</span>
                                    </div>                                
                                </div>
                            </div>
                        </header>
                    </div>
                    <div class="slds-card__body slds-card__body_inner slds-text-heading_small">
                        
                        <br/>
                        <p>Louisville Tile Distributors, Inc. (“LT”) is a fourth generation, family-owned company that has successfully operated for over 60 years as a respected leader in the tile industry.  In addition to its own branch and showrooms that sell directly to residential and commercial accounts it has a strong dealer network and markets directly to key accounts through a “National” Accounts program. The Company continues to grow both organically and through acquisitions with sales expected to eclipse $100 million in 2022. </p>
                        <br/>
                        <p>Louisville Tile currently has 12 showroom branches in 5 states (KY, OH, IN, MI, and TN) and expects to significantly grow that footprint over the next 5 years.  We have a central warehouse (“CW”) in Louisville, Kentucky and allow our vendors to ship supplies to either the CW or directly to our branch depending on the instructions from our logistics team. </p>
                        <br/>
                        <p>During the past 12 months LT has made significant investments in people, technology, facilities and other resources that will serve as the foundation of a significant strategic growth plan that has been put in place. It is expected that the company will experience a 15% CAGR over the next 5 years. LT expects to enter into distribution agreements with all Preferred Vendors awarded this status. These agreements will outline the details of our relationship including but not limited to LT minimum inventory carry and target annual purchase levels as well as Preferred Partner marketing, merchandising, territorial protection and service level criteria.  </p>
                        <br/>
                        <p>Our mission is to;</p>
                        <br/>
                        <p><i>“Provide a world class and innovative solution-oriented customer experience by retaining the most knowledgeable employee team with the highest integrity who create value for our customers and profitability for the company” </i></p>
                        <br/>
                        <p>LT is differentiating itself to its clientele and marketplace by providing a manager of managers value proposition and embracing its core competencies in product selection, design expertise, and quality assurance. To fulfill this mission and value proposition we are seeking a select group of 6-9 core preferred partners that meet our standards and will encompass our stock item ceramic sales. These sales represent approximately 80% or $45MM in 2022/3 aggregate purchase volume. We will work in partnership with 6-9 Partners that can work with us to attain our growth targets and who also meet the following minimum criteria:</p>
                        <br/>
                        
                        <p><b><u>Minimum Criteria for Preferred Partner Status:</u></b></p>
                        <br/>
                        <p><ul class="slds-list_dotted">
                            <li>Partner must have been in business for at least 5 years and have a strong balance sheet with sufficient unencumbered credit facilities to insure consistent supply</li>
                            <li>Partner must be able to provide 100% coverage to all LT Branches in all SKU’s selected by LT for distribution</li>
                            <li>Partner must have field support team including technical training</li>
                            <li>Partner must have vibrant sample program</li>
                            <li>Partner must participate in EDI and offer fully integrated purchasing solution </li>
                            <li>Partner must provide uniform pricing for all SKU’s selected by LT</li>
                            <li>Partner must be able to provide minimum 45 day terms or 2/10 net 30</li>
                            <li>Partner must enter into distribution agreement upon award </li>
                            <li>Partner must enter into mutual NDA and Confidentiality Agreement attached to this RFP</li>
                            <li>Partner must answer the attached RFP and make an in person presentation of capabilities and strategic plan to achieve mutual targets </li>
                            </ul>
                        </p>
                        <br/>
                        <p>Further, LT is seeking Partners that can provide a broad range of product and design choices. It reserves the right to choose multiple Partners or none.</p>
                        <br/>
                        <p>Compliance with LT distribution agreement, which will be provided at the in person presentation will be critical for any successful candidate(s). </p>
                        <br/>
                        <p><b>Deadline for submission is 5 business days prior to presentation unless otherwise excepted by LT</b></p>
                        <br/>
                        <p><ol class="slds-list_ordered">
                            <li>
                                Only organizations that meet the Minimum Criteria should apply.  Respondents that do not meet the criteria will not be considered as a Preferred Partner but may be considered as a supplier.
                            </li>
                            <li>
                                Respondents must respond to each item in Sections A through G.  Proposals should be prepared simply and economically, avoiding the use of elaborate promotional materials beyond those necessary to provide a complete, accurate and reliable presentation.  All responses should be brief and concise. The format of response must follow the format/order of the RFP precisely. The response should correlate 100% to the numbering and outline of the RFP.   
                            </li>
                            <li>
                                LT reserves the right, in its sole discretion, to reject any and all responses and to waive any irregularity or informality in any response. LT shall not be liable for any expenses and/or losses incurred by respondents.
                            </li>
                            <li>
                                No proposals or promotional materials will be returned. 
                            </li>
                            <li>
                                All questions should be emailed to Jenn VanDerVliet at the email below
                            </li>
                            </ol></p>
                        <br/>
                        <p>
                            To be considered please submit an electronic copy 5 business days prior to your presentation (unless excepted by LT) to: 
                        </p>
                        <p>Jenn VanDerVliet- Strategic Sourcing Manager</p>
                        <P><u>jvandervliet@louisville-tile.com</u></P>
                        
                        <br/>
                        <lightning:input class="slds-m-horizontal_small"
                                         aura:id="validField"
                                         type="checkbox"
                                         required="true"
                                         label="I have read and understood all terms and conditions."
                                         onchange="{!c.openModalForTermAndConditions}"/>
                    </div>
                    <footer class="slds-modal__footer">
                        <lightning:button variant="brand" name='Next' label='Next' onclick='{!c.hideModal}'/>
                    </footer>
                </article>
                
            </aura:if>
            <aura:if isTrue="{!v.sectionOpen}">
                <!--Start Section A: Profile of Manager and Firm-->
                <aura:if isTrue="{!v.nextPageName=='sectionA'}">
                    
                    <div class="slds-m-horizontal_large slds-m-vertical_small slds-text-heading_medium">
                        Section A: Profile of Manager and Firm
                    </div>
                    <!-- Start Accordion-->
                    <lightning:accordion allowMultipleSectionsOpen="true"                                
                                         activeSectionName="{! v.activeSections }">
                        <lightning:accordionSection name="firmNameAddress" label="Firm Name &#38; Address">
                            <!-- Firm Name -->
                            <div class="slds-grid slds-gutters slds-wrap slds-p-horizontal_small slds-p-top_xx-small">
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-m-bottom_xx-small">
                                    <lightning:input aura:id="pvDetails" 
                                                     class="checkValid"
                                                     type="text"
                                                     id="firmName"
                                                     label="Firm Name"
                                                     value="{!v.masterSectionAWrapper.firm.firmName}"
                                                     required="{!v.requiredForAll == true}"/>   	
                                </div>
                            </div>
                            
                            <!-- Firm Address -->
                            <div class="slds-grid slds-gutters slds-wrap slds-p-horizontal_small">
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_12-of-12">
                                    <div class="slds-text-heading_small slds-text-heading_small">
                                        <label Style = "color:red;"> *</label>Address 
                                    </div>
                                    <div class="slds-grid slds-gutters slds-wrap">
                                        <!-- Address and street -->
                                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-m-bottom_xx-small">
                                            <lightning:input aura:id = "pvDetails"
                                                             type ="text" 
                                                             label="Street Address with apt/suite #"
                                                             value="{!v.masterSectionAWrapper.firm.streetAddress}"
                                                             required="{!v.requiredForAll == true}"/>	
                                        </div>
                                        
                                        <!-- Attention to: -->
                                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-m-bottom_xx-small">
                                            <lightning:input type ="text" 
                                                             label="Attention to:"
                                                             value = "{!v.masterSectionAWrapper.firm.attentionTo}"/>	
                                        </div>
                                    </div>
                                    
                                    <div class="slds-grid slds-gutters slds-m-bottom_xx-small">
                                        <!-- City Name -->
                                        <div id="city" class="slds-col slds-size_1-of-1 slds-medium-size_4-of-12 slds-m-bottom_xx-small">
                                            <lightning:input aura:id = "pvDetails"
                                                             type ="text" 
                                                             label="City" 
                                                             class="city"
                                                             required="{!v.requiredForAll == true}"
                                                             value = "{!v.masterSectionAWrapper.firm.city}"/>	
                                        </div>
                                        <!-- state Name -->
                                        <div class="slds-col slds-size_1-of-1 slds-medium-size_4-of-12 slds-m-bottom_xx-small">
                                            <lightning:select aura:id = "pvDetails" name="select1" value="{!v.masterSectionAWrapper.firm.state}" label="State" 
                                                              required="{!v.requiredForAll == true}">
                                                <option value="">Choose State...</option>
                                                <aura:iteration items="{!v.lstState}" var="option">                                                
                                                    <option value="{!option}" 
                                                            selected="{!v.masterSectionAWrapper.firm.state == option}">{!option}</option>
                                                </aura:iteration>
                                            </lightning:select> 
                                        </div>
                                        
                                        <!-- Postal/ZIP Code -->
                                        <div class="slds-col slds-size_1-of-1 slds-medium-size_4-of-12 slds-m-bottom_xx-small">
                                            <lightning:input aura:id = "pvDetails"
                                                             type ="decimal"
                                                             maxlength = "5"
                                                             minlength = "5" 
                                                             label="Postal/ZIP Code"
                                                             required="{!v.requiredForAll == true}"
                                                             value = "{!v.masterSectionAWrapper.firm.postalCode}"/>	
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </lightning:accordionSection>
                        
                        <!-- Affiliates-->
                        <lightning:accordionSection name="affiliates" label="Affiliates (if any)">
                            <div class="slds-grid slds-gutters slds-wrap slds-p-horizontal_small slds-p-top_xx-small">
                                <!-- Affiliates Name -->
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-m-bottom_xx-small">
                                    <lightning:select aura:id = "pvDetails" name="Affiliate" value="{!v.masterSectionAWrapper.anyAffilate}"
                                                      label="Do you have any Affiliates?" required="{!v.requiredForAll == true}" >
                                        <option value="">choose one...</option>
                                        <aura:iteration items="{!v.picklistOptions}" var="option">
                                            <option value="{!option}" 
                                                    selected="{!v.masterSectionAWrapper.anyAffilate == option}">{!option}</option>
                                        </aura:iteration>                                    
                                    </lightning:select>
                                </div>
                                <aura:if isTrue="{!v.masterSectionAWrapper.anyAffilate == 'Yes'}">
                                    <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-m-bottom_xx-small">
                                        <div class="slds-float_right">
                                            <!-- Add More Sample Button -->
                                            <lightning:button label="Add More" 
                                                              iconName="utility:add" 
                                                              iconPosition="right" 
                                                              class="slds-m-vertical_small slds-m-horizontal_small" 
                                                              variant="brand" 
                                                              onclick ="{!c.addMoreAffiliates}"/>
                                        </div>
                                    </div>
                                </aura:if>
                            </div>
                            
                            <!-- Affiliates Address -->
                            <aura:if isTrue="{!v.masterSectionAWrapper.anyAffilate == 'Yes'}">
                                <lightning:accordion aura:id="accordion" activeSectionName="{!v.accordionName}">
                                    <aura:iteration  items="{!v.masterSectionAWrapper.affiliates}" var="Affiliates" indexVar="index">
                                        <aura:if isTrue="{!Affiliates.isDelete != true}">
                                            <span id ="{!'errorSection'+Affiliates.isError}">                                    
                                                <lightning:accordionSection name="{!(index+1)}" label="{!'Affiliate ' + (index+1)}">
                                                    <aura:set attribute="body">
                                                        <c:AffiliatesRecord aura:id="affiliate" Affiliates="{!Affiliates}"                                                                         
                                                                            deleteIndex ="{!index}"
                                                                            lstState="{!v.lstState}"
                                                                            isDeleteBtnDisabled="{!equals(v.masterSectionAWrapper.affiliates.length, 1)}"/>
                                                    </aura:set>
                                                </lightning:accordionSection>
                                            </span>
                                        </aura:if>
                                    </aura:iteration>
                                </lightning:accordion>
                            </aura:if>
                        </lightning:accordionSection>
                        
                        <!---Firm History-->
                        <lightning:accordionSection name="firmHistory" label="Firm History">
                            <div class="slds-grid slds-gutters slds-wrap slds-p-horizontal_small slds-p-top_xx-small">
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-m-bottom_xx-small">
                                    <lightning:select aura:id = "pvDetails" 
                                                      name="select1" 
                                                      value="{!v.masterSectionAWrapper.history.stateOfIncorporation}" 
                                                      label="State of incorporation (or country)" 
                                                      required="{!v.requiredForAll == true}">
                                        <option value="">Choose State...</option>
                                        <aura:iteration items="{!v.lstState}" var="option">                                                
                                            <option value="{!option}" 
                                                    selected="{!v.masterSectionAWrapper.history.stateOfIncorporation == option}">{!option}</option>
                                        </aura:iteration>                                    
                                    </lightning:select>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-m-bottom_xx-small">
                                    <lightning:input aura:id = "pvDetails" 
                                                     type="text"
                                                     label="Tax Id"
                                                     minlength="9"
                                                     maxlength="9"
                                                     value = "{!v.masterSectionAWrapper.history.taxId}"
                                                     required="{!v.requiredForAll == true}"/>
                                </div>
                            </div>
                            <div class="slds-grid slds-gutters slds-wrap slds-p-horizontal_small">
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-m-bottom_xx-small">
                                    <lightning:select aura:id = "pvDetails"
                                                      name="select1" 
                                                      label="Any customer or distributor legal suits currently pending or settled in past 5 years." 
                                                      value="{!v.masterSectionAWrapper.history.legalSuitePending}"
                                                      required="{!v.requiredForAll == true}">
                                        <option value="">choose one...</option>
                                        <aura:iteration items="{!v.picklistOptions}" var="option" >
                                            <option value="{!option}" 
                                                    selected="{!v.masterSectionAWrapper.history.legalSuitePending == option}">{!option}</option>
                                        </aura:iteration>
                                    </lightning:select>  	
                                </div>  
                                
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-m-bottom_xx-small">
                                    <lightning:select aura:id = "pvDetails"
                                                      name="select1" 
                                                      label="Any government legal action currently pending or settled in past 5 years."
                                                      value="{!v.masterSectionAWrapper.history.governmentLegalAction}"
                                                      required="{!v.requiredForAll == true}">
                                        <option value="">choose one...</option>
                                        <aura:iteration items="{!v.picklistOptions}" var="option">
                                            <option value="{!option}" 
                                                    selected="{!v.masterSectionAWrapper.history.governmentLegalAction == option}">{!option}</option>
                                        </aura:iteration>   
                                    </lightning:select>  	
                                </div>
                            </div>
                        </lightning:accordionSection>
                        
                        <!---Contact Information-->
                        <lightning:accordionSection name="contactInformation" label="Key Contact Professional(s)">
                            <div class="slds-grid slds-gutters slds-wrap slds-p-horizontal_small">
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_12-of-12">
                                    <div class="slds-card   slds-p-vertical_xx-small">  
                                        <lightning:accordion aura:id="accordion" activeSectionName="{!v.contactAccordionName}">
                                            <aura:iteration items="{!v.masterSectionAWrapper.keyContacts}" var="contact" indexVar="index">
                                                <aura:if isTrue="{!contact.isDelete != true}">
                                                    <span id ="{!'errorSection'+contact.isError}">
                                                        <lightning:accordionSection name="{!(index+1)}" label="{!'Key Contacts '  + (index+1)}">
                                                            <aura:set attribute="body">
                                                                <c:KeyContactInformation aura:id="keyContactChild"
