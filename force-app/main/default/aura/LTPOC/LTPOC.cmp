                                      class="slds-float_right slds-m-right_small slds-m-bottom_small" 
                                      name="Complete" 
                                      onclick="{!c.previousSection}" 
                                      variant="brand"/>
                </aura:if>
            </aura:if>
        </div>
        
        <!--Modal-->
        <aura:if isTrue="{!v.modalOpen}">
            <div role="dialog" class="slds-modal slds-fade-in-open ">
                <div class="slds-modal__container">
                    <header class="slds-modal__header  modal-header">
                        <h1 class="slds-text-heading--medium slds-text-color_inverse">Confirmation</h1>
                    </header>
                    <div class="slds-modal__content slds-p-around--medium  slds-text-heading_small">
                        <b><p align="center">Please note, once you submit the form, you will not be able to make any change.<br/>Do you want to submit ?</p></b>
                    </div>
                    <footer class="slds-modal__footer">
                        <lightning:button variant="brand" name='No' label='No' onclick='{!c.handleConfirmDialogNo}'/>
                        <lightning:button variant="brand" name='Yes' label='Yes' onclick='{!c.handleConfirmDialogYes}'/>
                    </footer>
                </div>
            </div>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </aura:if>
        <aura:set attribute="else">
            <c:LouisvilleTileLogoForVendor />
            <div class="slds-grid slds-gutters slds-wrap slds-p-horizontal_small slds-p-top_xx-small">                
                <c:LT_PreferredVendorRFPFormReview allSectionData ="{!v.allSectionData}"
                                                   allFileData ="{!v.allFileData}"/>
            </div>
        </aura:set>
    </aura:if>
</aura:component>