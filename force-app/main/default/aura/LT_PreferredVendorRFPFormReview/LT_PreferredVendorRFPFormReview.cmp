<aura:component Controller="LT_PreferredVendorRFPCtrl" 
                implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" 
                access="global" >
    <aura:attribute name ="preferredVendorObject" type="Object" />
    <aura:attribute name="activeSections" type="List" default="['SectionA','affiliates']" /> 
    <aura:attribute name="allSectionData" type="object" />
    <aura:attribute name="allFileData" type="object"/>
    
    <aura:attribute name="sectionAWarehouse" type="List" default="{!v.allFileData.sectionAWarehouse}"/>
    <aura:attribute name="sectionAManufacturingDetails" type="List" default="{!v.allFileData.sectionAManufacturingDetails}"/>
    <aura:attribute name="sectionBDistributor" type="List" default="{!v.allFileData.sectionBDistributor}"/>
    <aura:attribute name="sectionBManufacturer" type="List" default="{!v.allFileData.sectionBManufacturer}"/>
    <aura:attribute name="sectionBBalanceSheet" type="List" default="{!v.allFileData.sectionBBalanceSheet}"/>
    <aura:attribute name="sectionBAgreement" type="List" default="{!v.allFileData.sectionBAgreement}"/>
    <aura:attribute name="sectionCStrategicPlanning" type="List" default="{!v.allFileData.sectionCStrategicPlanning}"/>
    
    <aura:attribute name="masterSectionAWrapper" type="LT_PreferredVendorRFPCtrl.MasterSectionAWrapper" default="{!v.allSectionData.masterSectionAWrapper}"/>
    <aura:attribute name="masterSectionBWrapper" type="LT_PreferredVendorRFPCtrl.MasterSectionBWrapper" default="{!v.allSectionData.masterSectionBWrapper}"/>
    <aura:attribute name="masterSectionCWrapper" type="LT_PreferredVendorRFPCtrl.MasterSectionCWrapper" default="{!v.allSectionData.masterSectionCWrapper}"/>
    <aura:attribute name="masterSectionDWrapper" type="LT_PreferredVendorRFPCtrl.MasterSectionDWrapper" default="{!v.allSectionData.masterSectionDWrapper}"/>
    <aura:attribute name="masterSectionEWrapper" type="LT_PreferredVendorRFPCtrl.MasterSectionEWrapper" default="{!v.allSectionData.masterSectionEWrapper}"/>
    <aura:attribute name="masterSectionFWrapper" type="LT_PreferredVendorRFPCtrl.MasterSectionFWrapper" default="{!v.allSectionData.masterSectionFWrapper}"/>
    
    <!--  page background color  -->
    <aura:html tag="style">
        html {
        background: #F8F9F9;
        }
    </aura:html>
    
    <!-- lightning:accordion -->
    <br/>
    
    <lightning:accordion aura:id="accordion" 
                         allowMultipleSectionsOpen="true" 
                         onsectiontoggle="{! c.handleSectionToggle }"
                         activeSectionName="A">
        <lightning:accordionSection name="A" label=" Section A: Profile of Manager and Firm">
            <aura:set attribute="body">
                <div class="slds-m-top_medium formmargin">
                    <!--Basic details fields   -->
                    <div class="slds-grid slds-wrap slds-m-top_x-small">
                        
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-2">
                                    <strong >Firm Name: </strong>&nbsp;<span>{!v.masterSectionAWrapper.firm.firmName}</span> 
                                </div>
                                
                            </div>
                        </div>
                        
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                    <strong >Street Address with apt/suite #:</strong>&nbsp;<span>{!v.masterSectionAWrapper.firm.streetAddress}</span>   
                                </div>
                                
                            </div>
                        </div>
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                    <strong >Attention to:</strong> &nbsp;<span>{!v.masterSectionAWrapper.firm.attentionTo}</span>   
                                </div>
                            </div>
                        </div>
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                    <strong >City:</strong>&nbsp; <span>{!v.masterSectionAWrapper.firm.city}</span>
                                </div>
                                
                            </div>
                        </div>
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                    <strong >State:</strong> &nbsp;<span>{!v.masterSectionAWrapper.firm.state}</span>    
                                </div>
                                
                            </div>
                        </div>
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                    <strong >Zip Code:</strong>  &nbsp; <span>{!v.masterSectionAWrapper.firm.postalCode}</span>   
                                </div>
                                
                            </div>
                        </div>
                    </div>
                </div>
                
                <!-- Affiliates Name &#38; Address -->
                <div class=" slds-m-top_x-large slds-text-heading_small">
                    <h2>
                        Affiliates Name &#38; Address
                    </h2>
                </div>
                <div class="slds-m-top_medium formmargin">
                    <!--Billing Address fields   -->
                    <div class="slds-grid slds-wrap slds-m-top_x-small">
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-2">
                                    <strong >Do you have any Affiliates? </strong>&nbsp;<span>{!v.masterSectionAWrapper.anyAffilate}</span> 
                                </div>
                            </div>
                        </div>
                        <aura:if isTrue="{!v.masterSectionAWrapper.anyAffilate == 'Yes'}">
                            <lightning:card  title="List of Affiliate" class="table_bg_color">
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-large-size_12-of-12">
                                        <table aria-multiselectable="true" class="slds-table slds-table_bordered slds-table_fixed-layout slds-table_resizable-cols principal-table table_bg_color" role="grid">
                                            <thead >
                                                <tr class="slds-line-height_reset">
                                                    <th class="border_bg_color" scope="col">
                                                        <div class="slds-truncate slds-form-element__label slds-p-around_x-small table_header_font" ><strong>Affiliate Name</strong></div>
                                                    </th>
                                                    
                                                    <th class="border_bg_color" scope="col">
                                                        <div class="slds-truncate slds-form-element__label slds-p-around_x-small table_header_font"  ><strong>Street Address with apt/suite #</strong></div>
                                                    </th>
                                                    
                                                    <th class="border_bg_color" scope="col">
                                                        <div class="slds-truncate slds-form-element__label slds-p-around_x-small table_header_font" ><strong>Attention to</strong></div>
                                                    </th>
                                                    
                                                    <th class="border_bg_color" scope="col">
                                                        <div class="slds-truncate slds-form-element__label slds-p-around_x-small table_header_font" ><strong>City</strong></div>
                                                    </th>
                                                    
                                                    <th  class="border_bg_color" scope="col">
                                                        <div class="slds-truncate slds-form-element__label slds-p-around_x-small table_header_font" >State</div>
                                                    </th>
                                                    
                                                    <th  class="border_bg_color" scope="col">
                                                        <div class="slds-truncate slds-form-element__label slds-p-around_x-small table_header_font" >Zip Code</div>
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <!--Aura:Iteration Start-->
                                                <aura:iteration items="{!v.masterSectionAWrapper.affiliates}" var="Affiliates" >
                                                    <tr class="slds-hint-parent" >
                                                        <td> {!Affiliates.af_Name} </td>
                                                        <td> {!Affiliates.af_streetAddress} </td>
                                                        <td> {!Affiliates.af_attentionTo} </td>
                                                        <td> {!Affiliates.af_city} </td>
                                                        <td> {!Affiliates.af_state} </td>
                                                        <td> {!Affiliates.af_postalCode} </td>
                                                    </tr>
                                                </aura:iteration>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </lightning:card>
                        </aura:if>
                    </div>
                </div>
                
                
                <!--Firm History Fields -->
                <div class=" slds-m-top_x-large slds-text-heading_small slds-p-horizontal_medium">
                    <h2>
                        Firm History
                    </h2>
                </div>
                <div class="slds-m-top_medium formmargin">
                    <!--Primary Contact fields   -->
                    <div class="slds-grid slds-wrap slds-m-top_x-small">
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_3-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1 ">
                                    <strong >State of incorporation (or country):</strong> &nbsp;<span>{!v.masterSectionAWrapper.history.stateOfIncorporation}</span>    
                                </div>
                                
                            </div>
                        </div>
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_3-of-12 slds-large-size_3-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                    <strong >Tax Id:</strong> &nbsp;<span>{!v.masterSectionAWrapper.history.taxId}</span>    
                                </div>
                                
                            </div>
                        </div>
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_5-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1 setBottomLabelMargin">
                                    <strong >Any customer or distributor legal suits currently pending or settled in past 5 years:</strong> &nbsp;<span>{!v.masterSectionAWrapper.history.legalSuitePending}</span>    
                                </div>
                                
                            </div>
                        </div>
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                    <strong >Any government legal action currently pending or settled in past 5 years:</strong> &nbsp;<span>{!v.masterSectionAWrapper.history.governmentLegalAction}</span>    
                                </div>
                            </div>
                        </div> 
                    </div>
                </div>
                
                <!-- Key Contact Professional(s) -->
                <div class=" slds-m-top_x-large slds-text-heading_small slds-p-horizontal_medium">
                    <h2>
                        Key Contact Professional(s)
                    </h2>
                </div>
                <div class="slds-m-top_medium formmargin">
                    <!--Primary Accounts fields   -->
                    <div class="slds-grid slds-wrap slds-m-top_x-small">
                        <lightning:card  title="List of Key Contact" class="table_bg_color">
                            <div class="slds-grid slds-wrap">
                                <div class="slds-large-size_12-of-12">
                                    <table aria-multiselectable="true" class="slds-table slds-table_bordered slds-table_fixed-layout slds-table_resizable-cols principal-table table_bg_color" role="grid">
                                        <thead >
                                            <tr class="slds-line-height_reset">
                                                <th class="border_bg_color" scope="col">
                                                    <div class="slds-truncate slds-form-element__label slds-p-around_x-small table_header_font" ><strong>Who would be directly involved with servicing LT?</strong></div>
                                                </th>
                                                
                                                <th class="border_bg_color" scope="col">
                                                    <div class="slds-truncate slds-form-element__label slds-p-around_x-small table_header_font"><strong>First Name</strong></div>
                                                </th>
                                                
                                                <th class="border_bg_color" scope="col">
                                                    <div class="slds-truncate slds-form-element__label slds-p-around_x-small table_header_font" ><strong>Last Name</strong></div>
                                                </th>
                                                
                                                <th class="border_bg_color" scope="col">
                                                    <div class="slds-truncate slds-form-element__label slds-p-around_x-small table_header_font" ><strong>Phone</strong></div>
                                                </th>
                                                
                                                <th  class="border_bg_color" scope="col">
                                                    <div class="slds-truncate slds-form-element__label slds-p-around_x-small table_header_font" >Email</div>
                                                </th>                                                    
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <!--Aura:Iteration Start-->
                                            <aura:iteration items="{!v.masterSectionAWrapper.keyContacts}" var="Contacts" >
                                                <tr class="slds-hint-parent" >
                                                    <td> {!Contacts.kc_role} </td>
                                                    <td> {!Contacts.kc_firstName} </td>
                                                    <td> {!Contacts.kc_lastName} </td>
                                                    <td> {!Contacts.kc_phone} </td>
                                                    <td> {!Contacts.kc_email} </td>
                                                </tr>
                                            </aura:iteration>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </lightning:card>
                    </div>
                </div>
                
                <!--Warehouse-->
                <div class=" slds-m-top_x-large slds-text-heading_small slds-p-horizontal_medium">
                    <h2>
                        Warehouse
                    </h2>
                </div>
                <div class="slds-m-top_medium formmargin"> 
                    <div class="slds-grid slds-wrap slds-m-top_x-small">
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                    <strong>Do you have a detailed logistics and warehouse plan for LT and if no are you prepared to craft one if awarded Preferred Partner status?</strong><br/><span>{!v.masterSectionAWrapper.warehouse.isLogisticAndWarehousePlan}</span>   
                                </div>
                            </div>
                        </div>
                        <aura:if isTrue="{!v.masterSectionAWrapper.warehouse.isLogisticAndWarehousePlan == 'Yes'}">
                            <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                                <div class="slds-grid slds-gutters">
                                    <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                        <strong >Uploaded File</strong> <br/>
                                        <span><c:LT_PreferredVendorFileUploadDelete listFileUploaded ="{!v.sectionAWarehouse}" isSubmit="true"/></span>
                                    </div>
                                </div>
                            </div>
                        </aura:if>
                        <aura:if isTrue="{!v.masterSectionAWrapper.warehouse.isLogisticAndWarehousePlan == 'No'}">
                            <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                                <div class="slds-grid slds-gutters">
                                    <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                        <strong >Please specify the reason.</strong><br/><span>{!v.masterSectionAWrapper.warehouse.inputRemark}</span>  
                                    </div>
                                </div>
                            </div>
                        </aura:if>
                    </div>
                </div>
                
                <!--Dedicated Service Department-->
                <div class=" slds-m-top_x-large slds-text-heading_small slds-p-horizontal_medium">
                    <h2>
                        Dedicated Service Department
                    </h2>
                </div>
                <div class="slds-m-top_medium formmargin"> 
                    <div class="slds-grid slds-wrap slds-m-top_x-small">
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1 ">
                                    <strong >Is there a central ticket system or process for inquiry of issues?</strong> <br/><span>{!v.masterSectionAWrapper.serviceDepartment.centralTicketingSystem}</span>    
                                </div>
                            </div>
                        </div>
                        
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                    <strong >How are delivery delays communicated to LT?</strong><br/><span>{!v.masterSectionAWrapper.serviceDepartment.deliveryCommunication}</span>  
                                </div>
                            </div>
                        </div>
                        
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                    <strong >What is your corporate philosophy around service of accounts?</strong><br/><span>{!v.masterSectionAWrapper.serviceDepartment.corporatePhilosophy}</span>  
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
                <!--Affiliate companies dealing with dealers-->
                <div class=" slds-m-top_x-large slds-text-heading_small slds-p-horizontal_medium">
                    <h2>Affiliate companies dealing with dealers</h2>
                </div>
                <div class="slds-m-top_medium formmargin"> 
                    <div class="slds-grid slds-wrap slds-m-top_x-small">
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1 ">
                                    <strong >Do you or any of your affiliated companies sell directly to dealers or retail?</strong> 
                                    <br/>
                                    <span>{!v.masterSectionAWrapper.affiliateCompanies.directlySellDealer}</span>    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
                
                <!--Manufacturing Details-->
                <div class=" slds-m-top_x-large slds-text-heading_small slds-p-horizontal_medium">
                    <h2>Manufacturing Details</h2>
                </div>
                <div class="slds-m-top_medium formmargin"> 
                    <div class="slds-grid slds-wrap slds-m-top_x-small">
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1 ">
                                    <strong >What is your manufacturing capacity per day for any single SKU?</strong>
                                    <br/>
                                    <span>{!v.masterSectionAWrapper.manufacturingDetail.manufacturerCapacity}</span>    
                                </div>
                            </div>
                        </div>
                        
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                    <strong >Do you outsource any of your product to third party manufacturers?</strong>
                                    <br/>
                                    <span>{!v.masterSectionAWrapper.manufacturingDetail.thirdPartyRevenue}</span>  
                                </div>
                            </div>
                        </div>
                        <aura:if isTrue="{!v.masterSectionAWrapper.manufacturingDetail.thirdPartyRevenue == 'Yes'}">
                            <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                                <div class="slds-grid slds-gutters">
                                    <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                        <strong >What percentage of total revenue do they represent?</strong><br/><span>{!v.masterSectionAWrapper.manufacturingDetail.percentTotalRevenue}</span>  
                                    </div>
                                </div>
                            </div>
                        </aura:if>
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                    <strong >Please provide a short video of your manufacturing facility.</strong><br/><span>{!v.masterSectionAWrapper.manufacturingDetail.manufacturingVideo}</span>  
                                </div>
                            </div>
                        </div>
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_4-of-12  slds-p-horizontal_medium  formmargin slds-border_bottom">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-p-around_xx-small slds-col slds-size_1-of-1">
                                    <strong >Uploaded Video</strong> 
                                    <br/>
                                    <span><c:LT_PreferredVendorFileUploadDelete listFileUploaded ="{!v.sectionAManufacturingDetails}" isSubmit="true"/></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
                
                <!--Experience with Partners & Insurance-->
